{
  "name": "circle-cts",
  "version": "1.0.8",
  "license": "MIT",
  "description": "Circle Reader Component Library",
  "sideEffects": [
    "*.css"
  ],
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Ranhe <wenguang.fe@gmail.com> (https://circlereader.com/)",
  "scripts": {
    "build:css": "node-sass -r src -o dist",
    "compile": "npx tsc -p tsconfig.json && tsccss -o dist",
    "build": "npm run build:css && npm run compile",
    "release": "npm run build && npm publish",
    "lint": "eslint src/**",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.1",
    "antd": "^5.15.0",
    "circle-ihk": "^1.0.8",
    "classnames": "^2.5.1",
    "rc-align": "^4.0.15",
    "rc-virtual-list": "^3.11.3",
    "react": "^18.2.0",
    "react-draggable": "^4.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lodash": "^4.17.21",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "tsccss": "^1.0.0",
    "typescript": "^5.1.6"
  }
}
